
public class LoopingConcept {

	public static void main(String[] args) {
		
		int i = 1;
		
		//1. While loop
		while(i<=1) {
			System.out.println(i);
			i++;
		}
		
		System.out.println("***************************");
		
		//2. For loop
		
		for(int j=1; j<=10; j==) {
			System.out.println(j);
		}
		
	}

}
